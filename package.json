{
  "name": "ss-pipe",
  "version": "1.0.9",
  "description": "ss-pipe line is a CI/CD workflow to produce screenshots for my projects.",
  "main": "takeScreenshot.js",
  "type": "module",
  "scripts": {
    "screenshot": "node ./index.js -t screenshot",
    "gif": "node ./index.js -t gif ",
    "mockup": "node ./index.js -t screenshot -m",
    "ss-take": "npm run screenshot",
    "gif-take-mobile": "npm run gif -- --device mobile",
    "gif-take-laptop": "npm run gif -- --device laptop",
    "mockup-mobile": "npm run mockup -- --device mobile",
    "mockup-laptop": "npm run mockup -- --device laptop",
    "mockup-gif-laptop": "npm run gif-take-laptop -- -m",
    "mockup-gif-mobile": "npm run gif-take-mobile -- -m"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/myusf01/ss-pipe.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/myusf01/ss-pipe/issues"
  },
  "homepage": "https://github.com/myusf01/ss-pipe#readme",
  "dependencies": {
    "canvas": "^2.9.0",
    "commander": "^8.3.0",
    "get-pixels": "^3.3.3",
    "gif-encoder": "^0.7.2",
    "gif-encoder-2": "^1.0.5",
    "jimp": "^0.16.1",
    "puppeteer": "^13.0.1"
  }
}
